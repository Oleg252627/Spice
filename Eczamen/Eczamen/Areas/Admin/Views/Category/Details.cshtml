@model Eczamen.Models.CategoryModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h2 class="text-info">Details category</h2>
<br />


    <div class="border backgroundWhite">
        <div class="form-group row">
            <div class="col-2"><label asp-for="Name" class="col-form-label"></label></div>
            <div class="col-5"><input asp-for="Name" class="form-control" readonly="readonly" /></div>
        </div>
        <div class="form-group row">
            <div class="col-5 offset-2">
                <div class="row">
                    <div class="col-6">
                        <partial name="_BacToEditButtonPartial" model="@Model.Id"/>
                    </div>
                    <div class="col-6">
                        <partial name="_BacToListButtonPartial" />
                    </div>
                </div>

            </div>
        </div>
    </div>
